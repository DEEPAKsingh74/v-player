{
	"compilerOptions": {
	  "target": "ES5",
	  "module": "ESNext",
	  "jsx": "react-jsx",
	  "lib": ["DOM", "ESNext"],
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "moduleResolution": "node",
	  "declaration": true,
	  "outDir": "dist"
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "dist"]
  }
  